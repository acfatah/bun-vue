      "path": "src/registry/blocks/sidebar-08/layouts/SidebarInsetLayout/SidebarInsetLayout.vue",
      "content": "<script setup lang=\"ts\">\nimport { Icon } from '@iconify/vue'\nimport { ref } from 'vue'\nimport {\n  Avatar,\n  AvatarFallback,\n  AvatarImage,\n} from '@/components/ui/avatar'\nimport {\n  Collapsible,\n  CollapsibleContent,\n  CollapsibleTrigger,\n} from '@/components/ui/collapsible'\nimport {\n  DropdownMenu,\n  DropdownMenuContent,\n  DropdownMenuGroup,\n  DropdownMenuItem,\n  DropdownMenuLabel,\n  DropdownMenuSeparator,\n  DropdownMenuShortcut,\n  DropdownMenuTrigger,\n} from '@/components/ui/dropdown-menu'\nimport { ScrollArea } from '@/components/ui/scroll-area'\nimport {\n  Sidebar,\n  SidebarContent,\n  SidebarFooter,\n  SidebarGroup,\n  SidebarGroupLabel,\n  SidebarHeader,\n  SidebarInset,\n  SidebarMenu,\n  SidebarMenuAction,\n  SidebarMenuButton,\n  SidebarMenuItem,\n  SidebarMenuSub,\n  SidebarMenuSubButton,\n  SidebarMenuSubItem,\n  SidebarProvider,\n} from '@/components/ui/sidebar'\n\nimport Header from './Header.vue'\nimport { sidebarMenu } from './sidebar-menu'\n\nconst activeTeam = ref(sidebarMenu.teams[0])\n\nfunction setActiveTeam(team: typeof sidebarMenu.teams[number]) {\n  activeTeam.value = team\n}\n</script>\n\n<template>\n  <SidebarProvider>\n    <Sidebar variant=\"inset\">\n      <SidebarHeader>\n        <SidebarMenu>\n          <SidebarMenuItem>\n            <DropdownMenu>\n              <DropdownMenuTrigger as-child>\n                <SidebarMenuButton\n                  size=\"lg\"\n                  class=\"\n                    data-[state=open]:bg-sidebar-accent\n                    data-[state=open]:text-sidebar-accent-foreground\n                  \"\n                >\n                  <div\n                    class=\"\n                      flex aspect-square size-8 items-center justify-center rounded-lg\n                      bg-sidebar-primary text-sidebar-primary-foreground\n                    \"\n                  >\n                    <component :is=\"activeTeam.logo\" class=\"size-4\" />\n                  </div>\n                  <div class=\"grid flex-1 text-left text-sm leading-tight\">\n                    <span class=\"truncate font-semibold\">{{ activeTeam.name }}</span>\n                    <span class=\"truncate text-xs\">{{ activeTeam.plan }}</span>\n                  </div>\n                  <Icon icon=\"lucide:chevrons-up-down\" class=\"ml-auto\" />\n                </SidebarMenuButton>\n              </DropdownMenuTrigger>\n              <DropdownMenuContent\n                class=\"w-[--reka-dropdown-menu-trigger-width] min-w-56 rounded-lg\"\n                align=\"start\"\n                side=\"bottom\"\n                :side-offset=\"4\"\n              >\n                <DropdownMenuLabel class=\"text-xs text-muted-foreground\">\n                  Teams\n                </DropdownMenuLabel>\n                <DropdownMenuItem\n                  v-for=\"(team, index) in sidebarMenu.teams\"\n                  :key=\"team.name\"\n                  class=\"gap-2 p-2\"\n                  @click=\"setActiveTeam(team)\"\n                >\n                  <div class=\"flex size-6 items-center justify-center rounded-sm border\">\n                    <component :is=\"team.logo\" class=\"size-4 shrink-0\" />\n                  </div>\n                  {{ team.name }}\n                  <DropdownMenuShortcut>âŒ˜{{ index + 1 }}</DropdownMenuShortcut>\n                </DropdownMenuItem>\n                <DropdownMenuSeparator />\n                <DropdownMenuItem class=\"gap-2 p-2\">\n                  <div\n                    class=\"flex size-6 items-center justify-center rounded-md border bg-background\"\n                  >\n                    <Icon icon=\"lucide:plus\" class=\"size-4\" />\n                  </div>\n                  <div class=\"font-medium text-muted-foreground\">\n                    Add team\n                  </div>\n                </DropdownMenuItem>\n              </DropdownMenuContent>\n            </DropdownMenu>\n          </SidebarMenuItem>\n        </SidebarMenu>\n      </SidebarHeader>\n      <SidebarContent>\n        <SidebarGroup>\n          <SidebarGroupLabel>Platform</SidebarGroupLabel>\n          <SidebarMenu>\n            <Collapsible\n              v-for=\"item in sidebarMenu.navMain\"\n              :key=\"item.title\"\n              as-child\n              :default-open=\"item.isActive\"\n              class=\"group/collapsible\"\n            >\n              <SidebarMenuItem>\n                <CollapsibleTrigger as-child>\n                  <SidebarMenuButton tooltip=\"item.title\">\n                    <component :is=\"item.icon\" />\n                    <span>{{ item.title }}</span>\n                    <Icon\n                      icon=\"lucide:chevron-right\"\n                      class=\"\n                        ml-auto transition-transform duration-200\n                        group-data-[state=open]/collapsible:rotate-90\n                      \"\n                    />\n                  </SidebarMenuButton>\n                </CollapsibleTrigger>\n                <CollapsibleContent>\n                  <SidebarMenuSub>\n                    <SidebarMenuSubItem\n                      v-for=\"subItem in item.items\"\n                      :key=\"subItem.title\"\n                    >\n                      <SidebarMenuSubButton as-child>\n                        <a :href=\"subItem.url\">\n                          <span>{{ subItem.title }}</span>\n                        </a>\n                      </SidebarMenuSubButton>\n                    </SidebarMenuSubItem>\n                  </SidebarMenuSub>\n                </CollapsibleContent>\n              </SidebarMenuItem>\n            </Collapsible>\n          </SidebarMenu>\n        </SidebarGroup>\n        <SidebarGroup class=\"group-data-[collapsible=icon]:hidden\">\n          <SidebarGroupLabel>Projects</SidebarGroupLabel>\n          <SidebarMenu>\n            <SidebarMenuItem\n              v-for=\"item in sidebarMenu.projects\"\n              :key=\"item.name\"\n            >\n              <SidebarMenuButton as-child>\n                <a :href=\"item.url\">\n                  <component :is=\"item.icon\" />\n                  <span>{{ item.name }}</span>\n                </a>\n              </SidebarMenuButton>\n              <DropdownMenu>\n                <DropdownMenuTrigger as-child>\n                  <SidebarMenuAction show-on-hover>\n                    <Icon icon=\"lucide:ellipsis-vertical\" />\n                    <span class=\"sr-only\">More</span>\n                  </SidebarMenuAction>\n                </DropdownMenuTrigger>\n                <DropdownMenuContent class=\"w-48 rounded-lg\" side=\"bottom\" align=\"end\">\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:folder\" class=\"text-muted-foreground\" />\n                    <span>View Project</span>\n                  </DropdownMenuItem>\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:share\" class=\"text-muted-foreground\" />\n                    <span>Share Project</span>\n                  </DropdownMenuItem>\n                  <DropdownMenuSeparator />\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:trash\" class=\"text-muted-foreground\" />\n                    <span>Delete Project</span>\n                  </DropdownMenuItem>\n                </DropdownMenuContent>\n              </DropdownMenu>\n            </SidebarMenuItem>\n            <SidebarMenuItem>\n              <SidebarMenuButton class=\"text-sidebar-foreground/70\">\n                <Icon icon=\"lucide:ellipsis\" class=\"text-sidebar-foreground/70\" />\n                <span>More</span>\n              </SidebarMenuButton>\n            </SidebarMenuItem>\n          </SidebarMenu>\n        </SidebarGroup>\n      </SidebarContent>\n      <SidebarFooter>\n        <SidebarMenu>\n          <SidebarMenuItem>\n            <DropdownMenu>\n              <DropdownMenuTrigger as-child>\n                <SidebarMenuButton\n                  size=\"lg\"\n                  class=\"\n                    data-[state=open]:bg-sidebar-accent\n                    data-[state=open]:text-sidebar-accent-foreground\n                  \"\n                >\n                  <Avatar class=\"size-8 rounded-lg\">\n                    <AvatarImage :src=\"sidebarMenu.user.avatar\" :alt=\"sidebarMenu.user.name\" />\n                    <AvatarFallback class=\"rounded-lg\">\n                      {{ sidebarMenu.user.name.split(' ').slice(0, 2).join('') }}\n                    </AvatarFallback>\n                  </Avatar>\n                  <div class=\"grid flex-1 text-left text-sm leading-tight\">\n                    <span class=\"truncate font-semibold\">{{ sidebarMenu.user.name }}</span>\n                    <span class=\"truncate text-xs\">{{ sidebarMenu.user.email }}</span>\n                  </div>\n                  <Icon icon=\"lucide:chevrons-up-down\" class=\"ml-auto\" />\n                </SidebarMenuButton>\n              </DropdownMenuTrigger>\n              <DropdownMenuContent\n                class=\"w-[--reka-dropdown-menu-trigger-width] min-w-56 rounded-lg\" side=\"bottom\" align=\"end\" :side-offset=\"4\"\n              >\n                <DropdownMenuLabel class=\"p-0 font-normal\">\n                  <div class=\"flex items-center gap-2 px-1 py-1.5 text-left text-sm\">\n                    <Avatar class=\"size-8 rounded-lg\">\n                      <AvatarImage :src=\"sidebarMenu.user.avatar\" :alt=\"sidebarMenu.user.name\" />\n                      <AvatarFallback class=\"rounded-lg\">\n                        CN\n                      </AvatarFallback>\n                    </Avatar>\n                    <div class=\"grid flex-1 text-left text-sm leading-tight\">\n                      <span class=\"truncate font-semibold\">{{ sidebarMenu.user.name }}</span>\n                      <span class=\"truncate text-xs\">{{ sidebarMenu.user.email }}</span>\n                    </div>\n                  </div>\n                </DropdownMenuLabel>\n                <DropdownMenuSeparator />\n                <DropdownMenuGroup>\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:sparkles\" />\n                    Upgrade to Pro\n                  </DropdownMenuItem>\n                </DropdownMenuGroup>\n                <DropdownMenuSeparator />\n                <DropdownMenuGroup>\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:badge-check\" />\n                    Account\n                  </DropdownMenuItem>\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:credit-card\" />\n                    Billing\n                  </DropdownMenuItem>\n                  <DropdownMenuItem>\n                    <Icon icon=\"lucide:bell\" />\n                    Notifications\n                  </DropdownMenuItem>\n                </DropdownMenuGroup>\n                <DropdownMenuSeparator />\n                <DropdownMenuItem>\n                  <Icon icon=\"lucide:log-out\" />\n                  Log out\n                </DropdownMenuItem>\n              </DropdownMenuContent>\n            </DropdownMenu>\n          </SidebarMenuItem>\n        </SidebarMenu>\n      </SidebarFooter>\n    </Sidebar>\n    <SidebarInset>\n      <Header />\n      <ScrollArea\n        class=\"\n          max-h-[calc(100svh-4.5rem)] bg-background px-4\n          md:max-h-[calc(100vh-5.5rem)]\n        \"\n      >\n        <RouterView />\n      </ScrollArea>\n    </SidebarInset>\n  </SidebarProvider>\n</template>\n",
      "type": "registry:file",
      "target": "~/src/layouts/SidebarInsetLayout/SidebarInsetLayout.vue"
    },

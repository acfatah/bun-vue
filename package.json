{
  "name": "shadcn-vue",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "lint": "bunx --bun eslint",
    "lint:changed": "bunx --bun eslint --pass-on-no-patterns --no-warn-ignored $(git diff --name-only --diff-filter=d HEAD)",
    "lint:staged": "bunx --bun eslint --pass-on-no-patterns --no-warn-ignored $(git diff --cached --name-only --diff-filter=d)",
    "lint:inspect": "bunx --bun eslint --inspect-config",
    "format": "changed_files=$(git diff --name-only --diff-filter=d HEAD); bunx --bun eslint --fix $changed_files",
    "format:staged": "staged_files=$(git diff --cached --name-only --diff-filter=d); bunx --bun eslint --fix --pass-on-no-patterns --no-warn-ignored $staged_files && git add $staged_files",
    "type-check": "bunx --bun vue-tsc --noEmit",
    "precommit": "bun lint:staged",
    "registry": "bun src/cli",
    "update-templates": "bun scripts/update-templates.ts"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@internationalized/date": "^3.8.2",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-table": "^8.21.3",
    "@unovis/ts": "^1.6.0",
    "@unovis/vue": "^1.6.0",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/components": "^13.7.0",
    "@vueuse/core": "^13.7.0",
    "@vueuse/integrations": "^13.7.0",
    "@vueuse/router": "^13.7.0",
    "change-case": "^5.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "embla-carousel-vue": "^8.6.0",
    "es-toolkit": "^1.39.10",
    "mitt": "^3.0.1",
    "non.geist": "^1.0.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.4.1",
    "simple-git-hooks": "^2.13.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.8",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bun": "^1.2.20",
    "@types/common-tags": "^1.8.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.17.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "common-tags": "^1.8.2",
    "consola": "^3.4.2",
    "eslint": "^9.33.0",
    "eslint-plugin-better-tailwindcss": "^3.7.5",
    "eslint-plugin-format": "^1.0.1",
    "minimist": "^1.2.8",
    "oxc-parser": "^0.77.3",
    "pathe": "^2.0.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.12"
  },
  "trustedDependencies": ["@tailwindcss/oxide"],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "bun precommit",
    "commit-msg": "bun commitlint --edit ${1}"
  }
}
